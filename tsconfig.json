{
  "compilerOptions": {
    "target": "ES2019",
    "rootDir": "./src",
    "outDir": "./dist",
    "noEmit": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noUncheckedIndexedAccess": true,
    "skipLibCheck": true
  }
}
